<defauction>
    <name>CDA</name>
    <declare>
        <param>auction_bid_language</param>
        <value>pq</value>
    </declare>
    <declare>
        <param>bid_dominance_buy</param>
        <value>none</value>
    </declare>
    <declare>
        <param>bid_dominance_sell</param>
        <value>none</value>
    </declare>
    <declare>
        <param>bid_btq</param>
        <value>0</value>
    </declare>
    <declare>
        <param>bid_btq_strict</param>
        <value>0</value>
    </declare>
    <declare>
        <param>bid_btq_delta</param>
        <value>0</value>
    </declare>
    <declare>
        <param>matching_fn</param>
        <value>earliest</value>
    </declare>
    <declare>
        <param>pricing_k</param>
        <value>0</value>
    </declare>
    <declare>
        <param>sellerIDs</param>
        <value>SELLER_ID</value>
    </declare>
    <declare>
        <param>buyerIDs</param>
        <value>BUYER_ID</value>
    </declare>
    <trigger>
        <when>time = gameStartTime</when>
        <action>quote</action>
    </trigger>
    <trigger>
        <when>validBid</when>
        <action>clear</action>
        <action>quote</action>
    </trigger>
    [TRIGGERS]
</defauction>
